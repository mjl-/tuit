{
	"compilerOptions": {
		"declaration": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "Node",
		 "lib": ["DOM", "ES6", "DOM.Iterable", "ES2016"],
		"outDir": "."
	},
	"include": [
		"ts/*",
	],
	"exclude": [
	]
}
